{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "SignedData",
    "type": "object",
    "properties": {
        "encoding_method": {
            "type": "string",
            "maxLength": 36
        },
        "encoding_method_version": {
            "type": "integer"
        },
        "public_key": {
            "type": "string",
            "maxLength": 512
        },
        "signed_values": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "$ref": "https://raw.githubusercontent.com/kelseymok/ocpi/main/src/ocpi/schemas/v221/SignedValue.json"
            }
        },
        "url": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "encoding_method",
        "url",
        "signed_values"
    ]
}