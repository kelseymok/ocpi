                "maxLength": 36
            }
        },
        "last_updated": {
            "type": "string",
            "maxLength": 25
        },
        "max_electric_power": {
            "type": "integer"
        },
        "terms_and_conditions": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "id",
        "standard",
        "format",
        "power_type",
        "max_voltage",
        "max_amperage",
        "tariff_ids",
        "last_updated"
