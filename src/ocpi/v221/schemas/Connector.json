{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Connector",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "maxLength": 36
        },
        "standard": {
            "type": "string",
            "enum": [
                "CHADEMO",
                "DOMESTIC_A",
                "DOMESTIC_B",
                "DOMESTIC_C",
                "DOMESTIC_D",
                "DOMESTIC_E",
                "DOMESTIC_F",
                "DOMESTIC_G",
                "DOMESTIC_H",
                "DOMESTIC_I",
                "DOMESTIC_J",
                "DOMESTIC_K",
                "DOMESTIC_L",
                "IEC_60309_2_single_16",
                "IEC_60309_2_three_16",
                "IEC_60309_2_three_32",
                "IEC_60309_2_three_64",
                "IEC_62196_T1",
                "IEC_62196_T1_COMBO",
                "IEC_62196_T2",
                "IEC_62196_T2_COMBO",
                "IEC_62196_T3A",
                "IEC_62196_T3C",
                "PANTOGRAPH_BOTTOM_UP",
                "PANTOGRAPH_TOP_DOWN",
                "TESLA_R",
                "TESLA_S"
            ]
